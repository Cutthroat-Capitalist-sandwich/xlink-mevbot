@using NewLife;
@using NewLife.Web;
@using NewLife.Cube;
@using XCode;
@using xLink.Entity;
@{
    var fact = ViewBag.Factory as IEntityFactory;
    var page = ViewBag.Page as Pager;
}
@await Html.PartialAsync("_Area2", "")
<div class="form-group">
    <label for="productId" class="control-label">产品：</label>
    @Html.ForDropDownList("productId", Product.FindAllWithCache(), page["productId"], "全部", true)
</div>
<div class="form-group">
    <label for="version" class="control-label">版本：</label>
    @Html.ForDropDownList("version", Device.FindAllVersion(), page["version"], "全部", true)
</div>
@await Html.PartialAsync("_DateRange")